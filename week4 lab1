//question21
int main() {
    int month;
    cout << "Enter month number (1-12): ";
    cin >> month;

    switch (month) {
        case 1:
            cout << "In January there is:\n"
                 << "New Year, 1 January\n";
            break;
        case 2:
            cout << "In February there are no holidays. Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 3:
            cout << "In March there is:\n"
                 << "International Women's Day, 8 March\n"
                 << "Navruz, 21 March\n";
            break;
        case 4:
            cout << "In April there are no holidays. Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 5:
            cout << "In May there is:\n"
                 << "Memorial Day, 9 May\n";
            break;
        case 6:
            cout << "In June there are no holidays. Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 7:
            cout << "In July there are no holidays. Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 8:
            cout << "In August there are no holidays.Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 9:
            cout << "In September there is:\n"
                 << "Independence Day, 1 September\n";
            break;
        case 10:
            cout << "In October there is:\n"
                 << "Teachers Day, 1 October\n";
            break;
        case 11:
            cout << "In November there are no holidays.Ramadan Hayit and Kurban Hayit dates change every year.\n";
            break;
        case 12:
            cout << "In December there is:\n"
                 << "Constitution Day, 8 December\n";
            break;
        default:
            cout << "Invalid month number.\n";
    }
    return 0;
}
